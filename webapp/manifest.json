{
	"_version": "1.7.0",
	"sap.app": {
		"id": "joris.ide.myquotations2MyQuotations2",
		"type": "application",
		
		"i18n": "i18n/i18n.properties",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"sourceTemplate": {
			"id": "ui5template.basicSAPUI5ApplicationProject",
			"version": "1.40.12"
		},
		"dataSources": {
			"mainService": {
				"uri": "/",
				"type": "OData",
				"settings": {
					"odataVersion": "2.0",
					"localUri": "localService/metadata.xml"
				}
			}
		}
	},

	"sap.ui": {
		"technology": "UI5",
		"icons": {
			"icon": "",
			"favIcon": "",
			"phone": "",
			"phone@2": "",
			"tablet": "",
			"tablet@2": ""
		},
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		},
		"supportedThemes": [
			"sap_hcb",
			"sap_belize"

		]
	},

	"sap.ui5": {
		"rootView": {
			"viewName": "joris.ide.myquotations2MyQuotations2.view.App",
			"type": "XML"
		},
		"dependencies": {
			"minUI5Version": "1.30.0",
			"libs": {
				"sap.ui.core": {},
				"sap.m": {},
				"sap.ui.layout": {},
				"sap.ushell": {},
				"sap.collaboration": {},
				"sap.ui.comp": {},
				"sap.uxap": {},
				"sap.f": {}
			}
		},
		"config": {
			"sample": {
				"stretch": true,
				"files": [
					"Component.js",
					"App.controller.js",
					"App.view.xml",
					"Master.controller.js",
					"Master.view.xml",
					"Detail.controller.js",
					"Detail.view.xml",
					"DetailDetail.view.xml"
				]
			}
		},
		"contentDensities": {
			"compact": true,
			"cozy": true
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "joris.ide.myquotations2MyQuotations2.i18n.i18n"
				}
			},
			"": {
				"dataSource": "mainService"
			},
			"quotationModel": {
				"type": "sap.ui.model.json.JSONModel",
				"settings": {},
				"preload": false 
			},
			"selectedQuotationModel": {
				"type": "sap.ui.model.json.JSONModel",
				"settings": {},
				"preload": false 
			},
			"selectedItemModel": {
				"type": "sap.ui.model.json.JSONModel",
				"settings": {},
				"preload": false 
			}
		},
		"resources": {
			"css": [{
				"uri": "css/style.css"
			}]
		}
	}
}