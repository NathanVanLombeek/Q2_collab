<View xmlns="sap.m" displayBlock="true" controllerName="joris.ide.myquotations2MyQuotations2.controller.Master" height="100%">
	<Page id="masterPage" title="Master page">
		<List headerText="{i18n>headerText}" id="list" items="{Meetups}" noDataText="{i18n>noDataText}">
			<items>
				<ObjectListItem number="{ path: 'EventDate', type: 'sap.ui.model.type.DateTime', formatOptions: { style: 'medium' } }" title="{Title}">
					<attributes>
						<ObjectAttribute text="{Description}"/>
					</attributes>
				</ObjectListItem>
			</items>
		</List>
	
	</Page>
</View>